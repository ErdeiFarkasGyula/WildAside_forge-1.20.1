{
  "multipart": [
    {
      "when": {
        "is_yellow": "false"
      },
      "apply": {
        "model": "wildaside:block/hickory_fence_post"
      }
    },
    {
      "when": {
        "is_yellow": "true"
      },
      "apply": {
        "model":"wildaside:block/yellow_hickory_fence_post"
      }
    },
    {
      "when": {
        "north": "true",
        "is_yellow": "false"
      },
      "apply": {
        "model": "wildaside:block/hickory_fence",
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true",
        "is_yellow": "true"
      },
      "apply": {
        "model": "wildaside:block/yellow_hickory_fence",
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true",
        "is_yellow":"false"
      },
      "apply": {
        "model": "wildaside:block/hickory_fence",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true",
        "is_yellow":"true"
      },
      "apply": {
        "model": "wildaside:block/yellow_hickory_fence",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true",
        "is_yellow": "false"
      },
      "apply": {
        "model": "wildaside:block/hickory_fence",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true",
        "is_yellow": "true"
      },
      "apply": {
        "model": "wildaside:block/yellow_hickory_fence",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true",
        "is_yellow": "false"
      },
      "apply": {
        "model": "wildaside:block/hickory_fence",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true",
        "is_yellow": "true"
      },
      "apply": {
        "model": "wildaside:block/yellow_hickory_fence",
        "y": 90,
        "uvlock": true
      }
    }
  ]
}